{
  "expo": {
    "name": "Pick Games",
    "slug": "pick-games",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-icon.png",
    "splash": {
      "image": "./assets/pick-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2a2e33"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package" : "com.team2.pickgames",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-adaptive.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
